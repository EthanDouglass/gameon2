<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonGuestUser"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="50dp"
        android:fontFamily="@font/norwester"
        android:text="Guest"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/buttonNewUser"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="9dp"
        android:fontFamily="@font/norwester"
        android:text="New User"
        app:layout_constraintBottom_toTopOf="@+id/buttonReturningUser"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/buttonReturningUser"
        android:layout_width="250dp"
        android:layout_height="75dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="9dp"
        android:fontFamily="@font/norwester"
        android:text="Returning User"
        app:layout_constraintBottom_toTopOf="@+id/buttonGuestUser"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/GameOn_Logo"
        android:layout_width="441dp"
        android:layout_height="314dp"
        android:layout_marginTop="47dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gameon_final" />

    <Button
        android:id="@+id/buttonTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TestPage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>